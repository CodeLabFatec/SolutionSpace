{
  "name": "solutionspace",
  "version": "0.0.0",
  "description": "A simple TypeScript application to manage internal feature and hotfix demands.",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --noStackTrace --runInBand --no-cache",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "jest --runInBand --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeLabFatec/SolutionSpace.git"
  },
  "author": "CodeLabFatec",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeLabFatec/SolutionSpace/issues"
  },
  "homepage": "https://github.com/CodeLabFatec/SolutionSpace#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "eslint": "^8.35.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "git-commit-msg-linter": "^4.8.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
