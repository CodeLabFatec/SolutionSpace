{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "target": "ES6",
    "baseUrl": "src",
    "rootDirs": ["src"],
    "lib": ["dom", "dom.iterable", "esnext"],
    "isolatedModules": true,
    "jsx": "react-jsx",
    "outDir": "./dist",
    "paths": {
      "@/*": ["*"]
    }
  },
  "include": ["src"]
}
